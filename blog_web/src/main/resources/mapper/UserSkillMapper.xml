<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.red.mapper.UserSkillMapper">
    <select id="selectSkillIdByUserId" resultType="long">
        select skill_id
        from tb_user_skill
        where user_id = #{userId};
    </select>
</mapper>